{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2022",
		"lib": [
			"ES2022"
		],
		"rootDir": "src",
		"outDir": "out",
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true,
	}
}
